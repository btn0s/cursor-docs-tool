#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/shadcn@3.6.2_@types+node@20.19.27_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/shadcn@3.6.2_@types+node@20.19.27_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/node_modules:/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/shadcn@3.6.2_@types+node@20.19.27_hono@4.11.1_typescript@5.9.3/node_modules:/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/shadcn@3.6.2_@types+node@20.19.27_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/shadcn@3.6.2_@types+node@20.19.27_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/node_modules:/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/shadcn@3.6.2_@types+node@20.19.27_hono@4.11.1_typescript@5.9.3/node_modules:/Users/btn0s/dev/btn0s/2026/artifacts/cursor-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../shadcn/dist/index.js" "$@"
fi
